{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "MY_SECRET_KEY": "@my-secret-key"
  },
  "functions": {
    "server.js": {
      "maxDuration": 10,
      "memory": 512
    }
  }
}
